{
	"welcomeFile": "index.html",
	"authenticationMethod": "route",
	"routes": [{
		"source": "(.*)(.xsjs)",
		"destination": "core-backend",
		"csrfProtection": false,
		"authenticationType": "xsuaa"
	}, {
		"source": "(.*)(.xsodata)",
		"destination": "core-backend",
		"csrfProtection": false,
		"authenticationType": "xsuaa"
	}, {
		"source": "/(.*)",
		"localDir": "resources",
		"authenticationType": "xsuaa",
		"csrfProtection": false,
		"replace": {
			"pathSuffixes": ["index.html", "odataTest.html"],
			"vars": ["ui5liburl"]
		}
	}]
}